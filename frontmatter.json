{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "content",
      "path": "[[workspace]]/content"
    },
    {
      "title": "archetypes",
      "path": "[[workspace]]/archetypes"
    },
    {
      "title": "blowfish",
      "path": "[[workspace]]/themes/blowfish"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts"
    },
    {
      "title": "docs",
      "path": "[[workspace]]/content/docs"
    },
    {
      "title": "kalman",
      "path": "[[workspace]]/content/posts/kalman"
    },
    {
      "title": "archetypes",
      "path": "[[workspace]]/themes/blowfish/archetypes"
    },
    {
      "title": ".vscode",
      "path": "[[workspace]]/themes/blowfish/.vscode"
    },
    {
      "title": "knowledge",
      "path": "[[workspace]]/content/posts/knowledge"
    },
    {
      "title": "healthy",
      "path": "[[workspace]]/content/posts/healthy"
    },
    {
      "title": "cardep",
      "path": "[[workspace]]/content/posts/cardep"
    },
    {
      "title": "cabride",
      "path": "[[workspace]]/content/posts/cabride"
    },
    {
      "title": "accept",
      "path": "[[workspace]]/content/posts/accept"
    },
    {
      "title": "PiraeusHarbour",
      "path": "[[workspace]]/content/posts/PiraeusHarbour"
    },
    {
      "title": "MMAs",
      "path": "[[workspace]]/content/posts/MMAs"
    },
    {
      "title": "static",
      "path": "[[workspace]]/static"
    },
    {
      "title": "resources",
      "path": "[[workspace]]/resources"
    }
  ],
  "frontMatter.git.enabled": true
}